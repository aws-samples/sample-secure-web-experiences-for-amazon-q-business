{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "PolicyForAmazonQWebAccessForWebExperienceArtifacts",
      "Effect": "Allow",
      "Principal": {
        "Service": "application.qbusiness.amazonaws.com"
      },
      "Action": [
        "s3:GetObject"
      ],
      "Resource": [
        "arn:aws:s3:::amazon-q-business-ui-customizations/public-sector-theme.css",
        "arn:aws:s3:::amazon-q-business-ui-customizations/aws-logo.png",
        "arn:aws:s3:::amazon-q-business-ui-customizations/AmazonEmber_Bd.ttf",
        "arn:aws:s3:::amazon-q-business-ui-customizations/favicon.ico"
      ],
      "Condition": {
        "Bool": {
          "aws:SecureTransport": "true"
        }
      }
    }
  ]
}