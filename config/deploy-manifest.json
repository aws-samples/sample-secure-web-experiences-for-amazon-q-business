{
  "version": 1,
  "framework": { "name": "express", "version": "5.1.0" },
  "imageSettings": {
    "sizes": [
      100,
      200,
      1920
    ],
    "domains": [],
    "remotePatterns": [],
    "formats": [],
    "minimumCacheTTL": 60,
    "dangerouslyAllowSVG": false
  },
  "routes": [
    {
      "path": "/*",
      "target": {
        "kind": "Compute",
        "src": "default"
      }
    }
  ],
  "computeResources": [
    {
      "name": "default",
      "runtime": "nodejs20.x",
      "entrypoint": "index.js",
      "environmentVariables": {
        "SECRET_NAME": "${SECRET_NAME}",
        "REGION": "${REGION}",
        "SESSION_DURATION_MINUTES": "${SESSION_DURATION_MINUTES}",
        "NODE_ENV": "production"
      }
    }
  ]
}
